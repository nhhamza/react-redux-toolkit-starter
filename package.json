{
  "name": "webpack-5-react-typescript",
  "version": "2.0.0",
  "description": "A webpack 5  with React TypeScript",
  "main": "index.ts",
  "author": "",
  "license": "MIT",
  "scripts": {
    "start2": "webpack serve --config webpack/webpack.dev.js --progress",
    "start": "cross-env NODE_ENV=development ENVIRONMENT=lcl webpack-dev-server --open",
    "start:dev": "cross-env NODE_ENV=development ENVIRONMENT=dev webpack-dev-server --open",
    "build": "cross-env NODE_ENV=production ENVIRONMENT=lcl webpack",
    "build:open": "cross-env NODE_ENV=production ENVIRONMENT=lcl webpack-dev-server --open",
    "build:dev": "cross-env NODE_ENV=production ENVIRONMENT=dev webpack",
    "prod": "http-server ./dist -a localhost -p 8005 -o",
    "test": "jest --watch",
    "test:ci": "jest --coverage --silent --ci --env=jsdom",
    "prepare": "husky install",
    "lint": "eslint --fix",
    "pretty": "prettier --write .",
    "precommit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {
    "verbose": true,
    "roots": [
      "<rootDir>/src"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/>(*.)test.{ts, tsx}"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/public/"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --quiet --cache --fix"
    ],
    "*.{json,js,ts,jsx,tsx,html}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@types/node": "^16.11.63",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.8.4",
    "web-vitals": "^2.1.4",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/plugin-transform-arrow-functions": "^7.18.6",
    "@babel/plugin-transform-async-to-generator": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@soda/friendly-errors-webpack-plugin": "^1.8.1",
    "@testing-library/dom": "^8.18.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "core-js": "^3.25.5",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.2",
    "eslint": "^8.24.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^3.2.0",
    "fork-ts-checker-webpack-plugin": "^7.1.13",
    "html-webpack-plugin": "^5.0.0",
    "http-server": "14.1.1",
    "husky": "^8.0.0",
    "jest": "^29.1.2",
    "jest-dom": "^4.0.0",
    "jest-environment-jsdom": "^29.1.2",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "node-sass": "^7.0.3",
    "path": "^0.12.7",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.2",
    "prettier": "^2.7.1",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "typescript": "^4.8.4",
    "webpack": "5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  }
}
